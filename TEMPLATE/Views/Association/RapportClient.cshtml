@model IEnumerable<TEMPLATE.Models.recoltModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layouts/_Default.cshtml";
}
<div class="main-panel">
 <div class="container">

    <div class="panel-header bg-primary-gradient">
       <div class="page-inner py-3">
          <div class="d-flex align-items-left align-items-md-center flex-column flex-md-row">
             <div>
              <h2 class="text-white pb-2 fw-bold">
                Rapport
            </h2>
        </div>
        <div class="ml-md-auto py-2 py-md-0">
         <!-- PAGE-HEADER -->
         <div class="page-header">
            <ol class="breadcrumb">
               <li class="breadcrumb-item">
                  <a href="#">Association</a>
              </li>
              <li class="breadcrumb-item">
                <a href="#">Rapport</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page" >
              Client 
          </li>
      </ol>
  </div>
  <!-- PAGE-HEADER END -->
</div>
</div>
</div>
</div>
<div class="card-body rec-pat" >
  <div class="container-fluid">
      <div class="row">
    <div class="col-md-12" id="containerAssociationClients"></div>
    
</div>
      <div class="table-responsive">
        <table class="table table-striped  table-bordered  " id="mytable1"  style="width:100%">
           <thead>
               <tr style="background-color:darkturquoise">
                    <th>NO</th>
                    <th>NOM</th>
                    <th>MEMBRE</th>
              </tr>
           </thead>
            <tbody>
                @{ int NO = 0;}
                @foreach (var item in Model) 
                {
                   NO = NO + 1;
                   <tr>
                <td>
                    @NO 
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.count)
                </td>
                   
            </tr>
                }
            </tbody>
        </table></div>

 </div>
   </div>
</div>
@section scripts
{
    <script src="~/Scripts/ChartsAssociationClient.js"></script>
    <script src="~/Scripts/tableau.js"></script>
    <script src="~/Scripts/tableau1.js"></script>
}
