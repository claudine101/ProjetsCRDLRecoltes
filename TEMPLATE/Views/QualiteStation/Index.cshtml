@model IEnumerable<TEMPLATE.Models.qualite>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layouts/_DefaultStation.cshtml";
}
<div class="main-panel">
 <div class="container">

    <div class="panel-header bg-primary-gradient">
       <div class="page-inner py-3">
          <div class="d-flex align-items-left align-items-md-center flex-column flex-md-row">
             <div>
              <h2 class="text-white pb-2 fw-bold">
                ENREGISTREMENT DES RECOLTES DANS @Session["association"]
            </h2>
        </div>
        <div class="ml-md-auto py-2 py-md-0">
         <!-- PAGE-HEADER -->
         <div class="page-header">
            <ol class="breadcrumb">
               <li class="breadcrumb-item">
                  <a href="#">Home</a>
              </li>
              <li class="breadcrumb-item">
                <a href="#">Qualite</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">
              Liste 
          </li>
      </ol>
  </div>
  <!-- PAGE-HEADER END -->
</div>
</div>
</div>
</div>
   <div class="card-body rec-pat" >
      
       <div class="table-responsive">
          <table class="table table-striped  table-bordered  " id="mytable1"  style="width:100%">
              <thead>
                  <tr style="background-color:darkturquoise">
                    <th>NO</th>
                    <th>NOM</th>
                 </tr>
              </thead>
              <tbody>
                    @{ int NO=0;}
                    @foreach (var item in Model)
                    {
                        NO = NO + 1;
                        <tr>
                            <td>
                                @NO
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.NOM_qualite)
                            </td>
                        </tr>
                    }
              </tbody>
           </table>
      </div>
</div>
</div>


  @section scripts
    {
    <script src="~/Scripts/tables1.js"></script>
}




